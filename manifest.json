{
  "manifest_version": 2,
  "name": "Click To Contain",
  "version": "0.0.7alpha",
  "description": "Let's external links open in random containers",
  "author": "LoveIsGrief",
  "applications": {
    "gecko": {
      "id": "@click-to-contain"
    }
  },
  "icons": {
    "64": "resources/icon-256.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "js": [
        "randomize.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "<all_urls>",
    "activeTab",
    "cookies",
    "contextualIdentities",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "settings.html"
  }
}
